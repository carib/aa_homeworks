<h1>Add A New $#%!in' Album!</h1>
<form class="" action="<%= band_albums_url %>" method="post">
  <%= auth_token %>
  <input type="hidden" name="_method" value="patch">
  Band Name: <%= Band.find_by(params[:band_id]).name %>
  <br>
  <label>Album Title:
    <input type="text" name="album[title]" value="">
  </label>
  <br>
  <label>Year:
    <%= year_picker %>
  </label>
  <br>
  <label>
    <% %w(Live Studio) %>
    <input type="radio" name="album[live]" value="">
  </label>
</form>